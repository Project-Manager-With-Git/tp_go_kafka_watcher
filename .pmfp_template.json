{
    "language": "go",
    "env": "gomod",
    "description": "go语言监听kafka的项目模板,同时代发送端",
    "author": "hsz",
    "template_type": "watcher",
    "requires": [
        "github.com/Golang-Tools/loggerhelper/v2@v2.0.1",
        "github.com/Golang-Tools/schema-entry-go/v2@v2.1.0",
        "github.com/confluentinc/confluent-kafka-go@v1.8.2"
    ],
    "template_keys": {
        "app_name": {
            "description": "项目名",
            "default": "{{ project_name }}"
        }
    },
    "components": {
        "kafkasender_proxy": {
            "source": "kafkasender/sender.go.jinja",
            "description": "kafka的生产者代理模板",
            "default_path": "kafkasender/sender.go"
        },
        "kafkasender_app": {
            "source": "kafkasender/app.go.jinja",
            "description": "kafka的发送端应用模板",
            "default_path": "kafkasender/app.go"
        },
        "kafkawatcher_proxy": {
            "source": "kafkawatcher/watcher.go.jinja",
            "description": "kafka的消费者代理模板",
            "default_path": "kafkawatcher/watcher.go"
        },
        "kafkawatcher_handdler": {
            "source": "kafkawatcher/handdler.go.jinja",
            "description": "kafka的监听端处理程序模板",
            "default_path": "kafkawatcher/handdler.go"
        },
        "kafkawatcher_app": {
            "source": "kafkawatcher/app.go.jinja",
            "description": "kafka的监听端应用程序模板",
            "default_path": "kafkawatcher/app.go"
        },
        "main": {
            "source": "main.go.jinja",
            "description": "kafka监听项目的入口函数",
            "default_path": "{{ project_name }}.go"
        },
        "dockerfile": {
            "source": "dockerfile.jinja",
            "description": "用于打包镜像的dockerfile文件",
            "default_path": "dockerfile"
        },
        "dockercompose": {
            "source": "docker-compose.yml.jinja",
            "description": "用于测试程序的docker-compose文件",
            "default_path": "docker-compose.yml"
        }
    }
}